{
  "name": "wsdb-client-app",
  "version": "0.0.1",
  "description": "WebSocket DB Client",
  "author": "next0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "_____start_____": "",
    "start": "vite",
    "start:dev": "npm run start",
    "_____build_____": "",
    "build": "tsc && vite build",
    "build:clear": "rm -rf ./.cache ./node_modules/.cache ./__reports__ ./dist",
    "_____storybook_____": "",
    "storybook:start": "storybook dev -p 6006",
    "storybook:build": "storybook build -o ./dist/storybook",
    "_____lint_____": "",
    "lint": "run-s --continue-on-error lint:ts lint:typecheck",
    "lint:ts": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:typecheck": "tsc --noEmit -p tsconfig.json",
    "_____test_____": "",
    "test": "npm run test:unit",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest watch",
    "test:unit:ui": "vitest --ui",
    "test:visual": "npx playwright test",
    "test:visual:dev": "npx playwright test",
    "test:visual:ui": "npx playwright test --ui",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "_____fix_____": "",
    "fix": "run-s --continue-on-error fix:prettier fix:ts",
    "fix:ts": "npm run lint:ts -- --fix",
    "fix:prettier": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,json,yaml,yml}'"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.2",
