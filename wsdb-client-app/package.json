    "clsx": "2.0.0",
    "date-fns": "2.30.0",
    "react": "18.2.0",
    "react-day-picker": "8.9.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.49.2",
    "react-router-dom": "6.21.1",
    "tailwind-merge": "2.2.0",
    "tailwindcss-animate": "1.0.7",
    "web-vitals": "3.5.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "1.41.1",
    "@storybook/addon-essentials": "7.6.12",
